import math
a = float(input())
b = float(input())
c = float(input())

if (a+b<c):
    print("Invalid Triangle")
elif (b+c<a):
    print("Invalid Triangle")
elif (a+c<b):
    print("Invalid Triangle")
else:
    s = (a+b+c)/2
    area = math.sqrt(s*(s-a)*(s-b)*(s-c))
    print("The area of the triangle is:","{:.2f}".format(area))
